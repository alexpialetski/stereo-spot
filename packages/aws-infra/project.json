{
  "name": "aws-infra",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/aws-infra",
  "metadata": {
    "nx-terraform": {
      "projectType": "stateful"
    }
  },
  "targets": {
    "terraform-init": {
      "metadata": {
        "backendProject": "aws-infra-setup"
      }
    },
    "terraform-plan": {
      "dependsOn": ["terraform-init", "reassembly-trigger:build"]
    },
    "terraform-output": {
      "metadata": {
        "outputFormat": "env"
      }
    }
  }
}
