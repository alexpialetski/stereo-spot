{
  "name": "aws-adapters",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/aws-adapters",
  "implicitDependencies": ["shared-types"],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pip install -e ../shared-types -q && pip install -e . -q && pytest -v",
        "cwd": "{projectRoot}"
      },
      "configurations": {}
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ruff check src tests",
        "cwd": "{projectRoot}"
      },
      "configurations": {}
    }
  }
}
