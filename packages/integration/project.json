{
  "name": "integration",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/integration",
  "implicitDependencies": [
    "shared-types",
    "aws-adapters",
    "web-ui",
    "media-worker",
    "video-worker"
  ],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pytest -v",
        "cwd": "{projectRoot}"
      },
      "configurations": {},
      "dependsOn": ["stereo-spot:install-deps"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ruff check src tests",
        "cwd": "{projectRoot}"
      },
      "configurations": {}
    }
  }
}
